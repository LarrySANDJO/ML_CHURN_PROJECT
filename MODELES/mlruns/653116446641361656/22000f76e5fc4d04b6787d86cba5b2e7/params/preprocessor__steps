[('extra_services', ExtraServicesTransformer(service_cols=['Online Security', 'Online Backup',
                                       'Device Protection Plan',
                                       'Premium Tech Support', 'Streaming TV',
                                       'Streaming Movies', 'Streaming Music',
                                       'Unlimited Data'])), ('feature_engineering', FeatureEngineer()), ('frequency_encoding', FrequencyEncoder(columns=['Offer', 'Internet Type', 'City'])), ('outlier_clipping', OutlierClipper(columns=['Avg Monthly GB Download', 'Tenure in Months',
                        'Number of Referrals', 'Number of Dependents', 'Age',
                        'New_monthly_charge', 'Ratio_refund',
                        'Extra_charges_ratio', 'total_extra_services'])), ('column_transform', ColumnTransformer(transformers=[('num',
                                 Pipeline(steps=[('imputer',
                                                  SimpleImputer(strategy='median')),
                                                 ('scaler', MinMaxScaler())]),
                                 ['Avg Monthly GB Download', 'Tenure in Months',
                                  'Number of Referrals', 'Number of Dependents',
                                  'Age', 'New_monthly_charge', 'Ratio_refund',
                                  'Extra_charges_ratio',
                                  'total_extra_services']),
                                ('onehot',
                                 OneHotEncoder(handle_unknown='ignore'),
                                 ['Internet Service', 'Phone Service',
                                  'Multiple Lines', 'Married']),
                                ('ordinal',
                                 OrdinalEncoder(categories=[['Month-to-Month',
                                                             'One Year',
                                                             'Two Year']]),
                                 ['Contract'])]))]